<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="92.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.geekbrains.java.client.view.auth.AuthControl">
   <children>
      <VBox prefHeight="130.0" prefWidth="600.0">
         <children>
            <Label prefHeight="17.0" prefWidth="287.0" text="Логин" />
            <TextField fx:id="loginFild" />
            <Label text="Пароль" />
            <PasswordField fx:id="passFild" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="conectButton" mnemonicParsing="false" onAction="#authBut" text="Соединиться" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Отмена" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
